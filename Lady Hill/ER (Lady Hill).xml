                    <customFields>
                        <field>
                            <key>location</key>
                            <value>$$emoSignature_Location_value$$</value>
                        </field>
                        <field>
                            <key>complaintType</key>
                            <value>Customer Satisfaction Survey</value>
                        </field>
                        <field>
                            <key>rating</key>
                            <value>$$q8426_emote$$</value>
                        </field>
                        <field>
                            <key>comments</key>
                            <value>$$gp3700_c1_comment_value$$</value>
                        </field>
                        <field>
                            <key>roomNumber</key>
