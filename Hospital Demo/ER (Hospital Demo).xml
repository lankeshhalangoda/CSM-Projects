 <cepRule id="1" timeWindow="5">
        <params>
            <param>
                <name>percepticDrillDown</name>
                <operator>eq</operator>
                <value>"q9902"</value>
            </param>
            <joinOperator>and</joinOperator>
            <joinParam>
                <derivedValue>emoteNumericValue</derivedValue>
                <operator>ld</operator>
                <value>2</value>
            </joinParam>
        </params>
        <engagementActions>
            <action>
                <type>email</type>
                <data>
                    <emoSignatureBasedRecipientSelection>true</emoSignatureBasedRecipientSelection>
                    <emoSignatureBasedEmailRecipientList>
                        <recipient>
                            <emoSignatureCondition>
                                <Hospital>Medical</Hospital>
                                <Department>In-Patient</Department>
                            </emoSignatureCondition>
                             <to>lankeshh@emojot.com, leads@emojot.com</to>
                        </recipient>
                        <recipient>
                            <emoSignatureCondition>
                                <Hospital>Surgical</Hospital>
                                <Department>In-Patient</Department>
                            </emoSignatureCondition>
                             <to>lankeshh@emojot.com, leads@emojot.com</to>
                        </recipient>
                        <recipient>
                            <emoSignatureCondition>
                                <Department>In-Patient</Department>
                                <Hospital>Central</Hospital>
                            </emoSignatureCondition>
                             <to>lankeshh@emojot.com, leads@emojot.com</to>
                        </recipient>
                    </emoSignatureBasedEmailRecipientList>
                     <to>lankeshh@emojot.com, leads@emojot.com</to>
                    <subject>Alert! - Low NPS value</subject>
                    <bodyType>html</bodyType>
                    <body><![CDATA[<p>Hi, </p>
<p>Below patient has expressed low NPS value towards his/her experience at $$emoSignature_Hospital_value$$.</p>
<p>
Name - $$loyalty_Name_value$$
<br>
Telephone - $$loyalty_Telephone_value$$
<br>
Speciality - $$emoSignature_Speciality_value$$
<br>
Ward - $$emoSignature_Ward_value$$
<br>
Room Number - $$emoSignature_Room_value$$
<br>
NPS Rating - $$q9902_emote$$
<br>
Yours sincerely,
<br>
Team Emojot
<br>]]></body>
                </data>
                <validityPeriod>5</validityPeriod>
            </action>
        </engagementActions>
    </cepRule>
